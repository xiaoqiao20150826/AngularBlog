<div class="col-lg-7" id="center_frame" style="margin:0 0 0 20px">
<meta charset="utf-8">

<!-- blog insert form -->
 <form action="/board" method="post" enctype="multipart/form-data" name="writeForm" class="form-horizontal"  role="form">
   <div class="form-group">
      <label class="col-sm-2 control-label">
         Id
      </label>
      <div class="col-sm-10">
         <input name="userId" type="text" 
         value="<%= loginId%>" style="display:none">
         <input class="form-control" type="text" 
            value="<%= loginId%>" disabled>
      </div>
   </div>
   <div class="form-group">
      <label class="col-sm-2 control-label">
         Password
      </label>
      <div class="col-sm-10">
         <input class="form-control" name="password" type="password" 
            placeholder="password" autofocus="autofocus">
      </div>
   </div>
   <div class="form-group">
      <label class="col-sm-2 control-label">Title</label>
      <div class="col-sm-10">
         <input class="form-control" name="title" type="text" 
            placeholder="title"  >
      </div>
   </div>
   <div class="form-group">
      <label class="col-sm-2 control-label">Content</label>
      <div class="col-sm-10">
      <textarea class="col-sm-12" id="content_textarea" name="content" ></textarea><!--/textarea로 닫지않으면 아래 태그가 이상해짐  -->	
<!--          <input class="form-control" id="content" type="text"  -->
<!--             value="content"> -->
      </div>
   </div>
   <div class="form-group">
	    <label class="col-sm-2 control-label">File input</label>
	    <div class="col-sm-10">
	    	<input type="file" name="inputFile">
	    </div>
   </div>
   <button type="submit" class="btn btn-default pull-right">Submit</button>
</form>
</div>      
      
